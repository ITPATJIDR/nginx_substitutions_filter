{
  "realm": "myrealm",
  "enabled": true,
  "clients": [
    {
      "clientId": "oauth2-proxy-client",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "redirectUris": [
        "http://localhost:4180/oauth2/callback",
        "http://openresty/oauth2/callback",
        "http://localhost/oauth2/callback"
      ],
      "secret": "${OAUTH2_PROXY_CLIENT_SECRET}",
      "standardFlowEnabled": true
    }
  ],
  "users": [
    {
      "username": "testuser",
      "enabled": true,
      "email": "testuser@example.com",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ]
    }
  ]
}

